.spotlist .listnum {
  height: 69rpx;
  line-height: 69rpx;
  font-size: 27.6rpx;
  color: #fff;
  background: rgba(21, 21, 21, 0.3);
  text-align: center;
}
.spotlist .scrolllist {
  overflow: hidden;
  height: calc(100vh - 250rpx);
}
.spotlist .scrolllist .item {
  padding: 27.6rpx 41.4rpx;
  display: flex;
  position: relative;
  width: 100%;
  box-sizing: border-box;
}
.spotlist .scrolllist .item .imgbox {
  width: 220.8rpx;
  position: relative;
  margin-right: 27.6rpx;
}
.spotlist .scrolllist .item .imgbox .itemimg {
  width: 220.8rpx;
  height: 165.6rpx;
  display: block;
  border-radius: 14px;
}
.spotlist .scrolllist .item .imgbox .play {
  position: absolute;
  width: 69rpx;
  height: auto;
  right: 0;
  bottom: 0;
}
.spotlist .scrolllist .item .imgbox .imgbg {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.4);
  border-radius: 14px;
}
.spotlist .scrolllist .item .info {
  flex: 1;
  box-sizing: border-box;
}
.spotlist .scrolllist .item .info .title {
  font-size: 33.12rpx;
  line-height: 1;
  color: #fff;
  background-size: auto 33.12rpx;
  padding-left: 75.9rpx;
}
.spotlist .scrolllist .item .info .opentime {
  font-size: 27.6rpx;
  line-height: 1;
  color: #ABABAB;
  margin: 22.08rpx 0 16.56rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 414rpx;
}
.spotlist .scrolllist .item .info .range {
  font-size: 24.84rpx;
  line-height: 1;
  color: #ABABAB;
}
.spotlist .scrolllist .item .info .addree {
  font-size: 24.84rpx;
  line-height: 1;
  color: #fff;
  margin-top: 20.7rpx;
  display: flex;
}
.spotlist .scrolllist .item .info .addree image {
  height: 24.84rpx;
  width: 20.7rpx;
  margin-right: 13.8rpx;
}
.spotlist .scrolllist .item .info .tj {
  width: 80.04rpx;
  height: 35.88rpx;
  position: absolute;
  bottom: 35.88rpx;
  right: 41.4rpx;
  text-align: center;
  font-size: 24.84rpx;
  color: #00D998;
  line-height: 35.88rpx;
  background: rgba(0, 217, 152, 0.3);
  border-radius: 4px;
}
.spotlist.horizontal .listnum {
  height: 41.5rpx;
  line-height: 41.5rpx;
  font-size: 16.6rpx;
}
.spotlist.horizontal .scrolllist {
  height: calc(100vh - 150rpx);
}
.spotlist.horizontal .scrolllist .item {
  padding: 16.6rpx 24.9rpx;
}
.spotlist.horizontal .scrolllist .item .imgbox {
  width: 132.8rpx;
  margin-right: 16.6rpx;
}
.spotlist.horizontal .scrolllist .item .imgbox .itemimg {
  width: 132.8rpx;
  height: 99.6rpx;
}
.spotlist.horizontal .scrolllist .item .imgbox .play {
  width: 29.88rpx;
}
.spotlist.horizontal .scrolllist .item .info .title {
  font-size: 19.92rpx;
  background-size: auto 19.92rpx;
  padding-left: 45.65rpx;
}
.spotlist.horizontal .scrolllist .item .info .opentime {
  font-size: 16.6rpx;
  margin: 13.28rpx 0 9.96rpx;
  max-width: 415rpx;
}
.spotlist.horizontal .scrolllist .item .info .range {
  font-size: 14.94rpx;
}
.spotlist.horizontal .scrolllist .item .info .addree {
  font-size: 14.94rpx;
  margin-top: 12.45rpx;
}
.spotlist.horizontal .scrolllist .item .info .addree image {
  height: 14.94rpx;
  width: 12.45rpx;
  margin-right: 8.3rpx;
}
.spotlist.horizontal .scrolllist .item .info .tj {
  width: 48.14rpx;
  height: 21.58rpx;
  bottom: 21.58rpx;
  right: 24.9rpx;
  font-size: 14.94rpx;
  line-height: 21.58rpx;
  border-radius: 4px;
}
